    553          /***************************************************************************************************
    554           * @fn      SPIMgr_ZAppBufferLengthRegister
    555           *
    556           * @brief
    557           *
    558           * @param   maxLen - Max Length that the application wants at a time
    559           *
    560           * @return  None
    561           *
    562           ***************************************************************************************************/
    563          void MT_UartZAppBufferLengthRegister ( uint16 maxLen )
    564          {
    565            /* If the maxLen is larger than the RX buff, something is not right */
    566            if (maxLen <= MT_UART_DEFAULT_MAX_RX_BUFF)
    567              MT_UartMaxZAppBufLen = maxLen;
    568            else
    569              MT_UartMaxZAppBufLen = 1; /* default is 1 byte */
    570          }
    571          
    572          /***************************************************************************************************
    573           * @fn      SPIMgr_AppFlowControl
    574           *
    575           * @brief
    576           *
    577           * @param   status - ready to send or not
    578           *
    579           * @return  None
    580           *
    581           ***************************************************************************************************/
    582          void MT_UartAppFlowControl ( bool status )
    583          {
    584          
    585            /* Make sure only update if needed */
    586            if (status != MT_UartZAppRxStatus )
    587            {
    588              MT_UartZAppRxStatus = status;
    589            }
    590          
    591            /* App is ready to read again, ProcessZAppData have to be triggered too */
    592            if (status == MT_UART_ZAPP_RX_READY)
    593            {
    594              MT_UartProcessZAppData (MT_UART_DEFAULT_PORT, HAL_UART_RX_TIMEOUT );
    595            }
