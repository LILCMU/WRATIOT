    480          /***************************************************************************************************
    481           * @fn      MT_UartProcessZAppData
    482           *
    483           * @brief   | SOP | CMD  |   Data Length   | FSC  |
    484           *          |  1  |  2   |       1         |  1   |
    485           *
    486           *          Parses the data and determine either is SPI or just simply serial data
    487           *          then send the data to correct place (MT or APP)
    488           *
    489           * @param   port    - UART port
    490           *          event   - Event that causes the callback
    491           *
    492           *
    493           * @return  None
    494           ***************************************************************************************************/
    495          void MT_UartProcessZAppData ( uint8 port, uint8 event )
    496          {
    497          
    498            osal_event_hdr_t  *msg_ptr;
    499            uint16 length = 0;
    500            uint16 rxBufLen  = Hal_UART_RxBufLen(MT_UART_DEFAULT_PORT);
    501          
    502            /*
    503               If maxZAppBufferLength is 0 or larger than current length
    504               the entire length of the current buffer is returned.
    505            */
    506            if ((MT_UartMaxZAppBufLen != 0) && (MT_UartMaxZAppBufLen <= rxBufLen))
    507            {
    508              length = MT_UartMaxZAppBufLen;
    509            }
    510            else
    511            {
    512              length = rxBufLen;
    513            }
    514          
    515            /* Verify events */
    516            if (event == HAL_UART_TX_FULL)
    517            {
    518              // Do something when TX if full
    519              return;
    520            }
    521          
    522            if (event & ( HAL_UART_RX_FULL | HAL_UART_RX_ABOUT_FULL | HAL_UART_RX_TIMEOUT))
    523            {
    524              if ( App_TaskID )
    525              {
    526                /*
    527                   If Application is ready to receive and there is something
    528                   in the Rx buffer then send it up
    529                */
    530                if ((MT_UartZAppRxStatus == MT_UART_ZAPP_RX_READY ) && (length != 0))
    531                {
    532                  /* Disable App flow control until it processes the current data */
    533                   MT_UartAppFlowControl (MT_UART_ZAPP_RX_NOT_READY);
    534          
    535                  /* 2 more bytes are added, 1 for CMD type, other for length */
    536                  msg_ptr = (osal_event_hdr_t *)osal_msg_allocate( length + sizeof(osal_event_hdr_t) );
    537                  if ( msg_ptr )
    538                  {
    539                    msg_ptr->event = SPI_INCOMING_ZAPP_DATA;
    540                    msg_ptr->status = length;
    541          
    542                    /* Read the data of Rx buffer */
    543                    HalUARTRead( MT_UART_DEFAULT_PORT, (uint8 *)(msg_ptr + 1), length );
    544          
    545                    /* Send the raw data to application...or where ever */
    546                    osal_msg_send( App_TaskID, (uint8 *)msg_ptr );
    547                  }
    548                }
    549              }
    550            }
    551          }
