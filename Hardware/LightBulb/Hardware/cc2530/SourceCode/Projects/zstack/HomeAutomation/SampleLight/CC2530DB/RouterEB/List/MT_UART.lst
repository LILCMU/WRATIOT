    354           * @fn      MT_UartProcessZAppData
    355           *
    356           * @brief   | SOP | CMD  |   Data Length   | FSC  |
    357           *          |  1  |  2   |       1         |  1   |
    358           *
    359           *          Parses the data and determine either is SPI or just simply serial data
    360           *          then send the data to correct place (MT or APP)
    361           *
    362           * @param   port    - UART port
    363           *          event   - Event that causes the callback
    364           *
    365           *
    366           * @return  None
    367           ***************************************************************************************************/
    368          void MT_UartProcessZAppData ( uint8 port, uint8 event )
    369          {
    370          
    371            osal_event_hdr_t  *msg_ptr;
    372            uint16 length = 0;
    373            uint16 rxBufLen  = Hal_UART_RxBufLen(MT_UART_DEFAULT_PORT);
    374          
    375            /*
    376               If maxZAppBufferLength is 0 or larger than current length
    377               the entire length of the current buffer is returned.
    378            */
    379            if ((MT_UartMaxZAppBufLen != 0) && (MT_UartMaxZAppBufLen <= rxBufLen))
    380            {
    381              length = MT_UartMaxZAppBufLen;
    382            }
    383            else
    384            {
    385              length = rxBufLen;
    386            }
    387          
    388            /* Verify events */
    389            if (event == HAL_UART_TX_FULL)
    390            {
    391              // Do something when TX if full
    392              return;
    393            }
    394          
    395            if (event & ( HAL_UART_RX_FULL | HAL_UART_RX_ABOUT_FULL | HAL_UART_RX_TIMEOUT))
    396            {
    397              if ( App_TaskID )
    398              {
    399                /*
    400                   If Application is ready to receive and there is something
    401                   in the Rx buffer then send it up
    402                */
    403                if ((MT_UartZAppRxStatus == MT_UART_ZAPP_RX_READY ) && (length != 0))
    404                {
    405                  /* Disable App flow control until it processes the current data */
    406                   MT_UartAppFlowControl (MT_UART_ZAPP_RX_NOT_READY);
    407          
    408                  /* 2 more bytes are added, 1 for CMD type, other for length */
    409                  msg_ptr = (osal_event_hdr_t *)osal_msg_allocate( length + sizeof(osal_event_hdr_t) );
    410                  if ( msg_ptr )
    411                  {
    412                    msg_ptr->event = SPI_INCOMING_ZAPP_DATA;
    413                    msg_ptr->status = length;
    414          
    415                    /* Read the data of Rx buffer */
    416                    HalUARTRead( MT_UART_DEFAULT_PORT, (uint8 *)(msg_ptr + 1), length );
    417          
    418                    /* Send the raw data to application...or where ever */
    419                    osal_msg_send( App_TaskID, (uint8 *)msg_ptr );
    420                  }
    421                }
    422              }
    423            }
